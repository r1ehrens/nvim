{
    "terminal.integrated.fontFamily": "Hack Nerd Font",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "latex-workshop.view.pdf.viewer": "tab",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "search.useIgnoreFiles": false,



    "editor.suggestSelection": "first",
    // "editor.rulers": [80],
    // "editor.wordWrap": "wordWrapColumn",
    "editor.wrappingIndent": "same",
    // "editor.wordWrapColumn": 80,
    
    


    "vim.startofline": false,
    // "vim.textwidth": 80,
    "vim.hlsearch": true,
    "vim.vimrc.enable": true,
    "vim.easymotion": true,
    "vim.enableNeovim": true,
    "vim.neovimPath": "/usr/local/bin/nvim",
    "vim.insertModeKeyBindings": [
            {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
],
"vim.visualModeKeyBindingsNonRecursive": [
    {
        "before": ["<space>"],
        "commands": ["whichkey.show"]
    },
    {
        "before": [">"],
        "after": [">","g","v"]
    },
    {
        "before": ["<"],
        "after": ["<", "g","v"]
    }
],
"vim.normalModeKeyBindingsNonRecursive": [
    {
        "before": ["<space>"],
        "commands": ["whichkey.show"]
    },
    {
        "before": ["j"],
        "after": ["g", "j"]
    },
    {
        "before": ["k"],
        "after": ["g", "k"]
    },
    {
        "before": [ "-" ],
        "after": [ "/" ]
    },
    {
        "before": ["y","p"],
        "after": ["\"","0","p"]
    },
    {
        "before": ["g","d"],
        "commands": ["editor.action.revealDefinition"]
    },
    {
        "before": ["g","f"],
        "commands": ["editor.action.openLink"]
    },
    {
        "before": ["]","g"],
        "commands": ["editor.action.marker.next"]
    
    },
    {
        "before": ["[","g"],
        "commands": ["editor.action.marker.prev"]
    
    },
    {
        "before": [">"],
        "commands": ["editor.action.indentLines"]
    },
    {
        "before": ["<"],
        "commands": ["editor.action.outdentLines"]
    }
],
"vim.highlightedyank.enable": true,
"vim.searchHighlightTextColor": "rgb(0,0,0)",
"vim.searchHighlightColor": "rgb(255,255,0)",
"vim.showMarksInGutter": true,
"vim.showmodename": true,
"vim.useSystemClipboard": true,
"vim.visualstar": true,


"whichkey.bindings": [
    {
        "key": ".",
        "name": "settings",
        "type": "command",
        "command": "workbench.action.openSettingsJson"
    },
    {
        "key": "e",
        "name": "toggle explorer",
        "type": "command",
        "command": "workbench.view.explorer"
    },
    {
        "key": "p",
        "name": "preview",
        "type": "command",
        "command": "markdown.showPreviewToSide",
        "when": "!notebookEditorFocused && editorLangId == 'markdown'"
    },
    {
        "key": "q",
        "name": "close",
        "type": "command",
        "command": "workbench.action.closeEditorsInGroup",
    },
    {
        "key": "t",
        "name": "toggle explorer",
        "type": "command",
        "command": "workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "z",
        "name": "zen",
        "type": "command",
        "command": "workbench.action.toggleZenMode"
    },
    {
        "key": "b",
        "name": "+BUFFERS...",
        "type": "bindings",
        "bindings": [
            {
                "key": "b",
                "name": "show all buffers/editors",
                "type": "command",
                "command": "workbench.action.showAllEditors"
            },
            {
                "key": "d",
                "name": "close active editor",
                "type": "command",
                "command": "workbench.action.closeActiveEditor"
            },
            {
                "key": "h",
                "name": "move editor into left group",
                "type": "command",
                "command": "workbench.action.moveEditorToLeftGroup"
            },
            {
                "key": "j",
                "name": "move editor into below group",
                "type": "command",
                "command": "workbench.action.moveEditorToBelowGroup"
            },
            {
                "key": "k",
                "name": "move editor into above group",
                "type": "command",
                "command": "workbench.action.moveEditorToAboveGroup"
            },
            {
                "key": "l",
                "name": "move editor into right group",
                "type": "command",
                "command": "workbench.action.moveEditorToRightGroup"
            },
            {
                "key": "n",
                "name": "new untitled editor",
                "type": "command",
                "command": "workbench.action.files.newUntitledFile"
            },
        ]
    },
    {
        "key": "d",
        "name": "+DEBUG...",
        "type": "bindings",
        "bindings": [
            {
                "key": "b",
                "name": "toggle breakpoint",
                "type": "command",
                "command": "editor.debug.action.toggleBreakpoint"
            },
            {
                "key": "B",
                "name": "toggle inline breakpoint",
                "type": "command",
                "command": "editor.debug.action.toggleInlineBreakpoint"
            },
            {
                "key": "c",
                "name": "continue debug",
                "type": "command",
                "command": "workbench.action.debug.continue"
            },
            {
                "key": "d",
                "name": "start debug",
                "type": "command",
                "command": "workbench.action.debug.start"
            },
            {
                "key": "D",
                "name": "run without debugging",
                "type": "command",
                "command": "workbench.action.debug.run"
            },
            {
                "key": "e",
                "name": "list errors",
                "type": "command",
                "command": "workbench.actions.view.problems"
            },
            {
                "key": "i",
                "name": "step into",
                "type": "command",
                "command": "workbench.action.debug.stepInto"
            },
            {
                "key": "j",
                "name": "jump to cursor",
                "type": "command",
                "command": "debug.jumpToCursor"
            },
            {
                "key": "o",
                "name": "step out",
                "type": "command",
                "command": "workbench.action.debug.stepOut"
            },
            {
                "key": "p",
                "name": "pause debug",
                "type": "command",
                "command": "workbench.action.debug.pause"
            },
            {
                "key": "r",
                "name": "restart debug",
                "type": "command",
                "command": "workbench.action.debug.restart"
            },
            {
                "key": "s",
                "name": "step over",
                "type": "command",
                "command": "workbench.action.debug.stepOver"
            },
            {
                "key": "S",
                "name": "stop debug",
                "type": "command",
                "command": "workbench.action.debug.stop"
            },
            {
                "key": "w",
                "name": "focus on watch window",
                "type": "command",
                "command": "workbench.debug.action.focusWatchView"
            },
            {
                "key": "W",
                "name": "add to watch",
                "type": "command",
                "command": "editor.debug.action.selectionToWatch"
            }
        ]
    },
    {
        "key": "g",
        "name": "+GIT...",
        "type": "bindings",
        "bindings": [
            {
                "key": "a",
                "name": "add",
                "type": "command",
                "command": "git.stage"
            },
            {
                "key": "b",
                "name": "checkout",
                "type": "command",
                "command": "git.checkout"
            },
            {
                "key": "c",
                "name": "commit",
                "type": "command",
                "command": "git.commit"
            },
            {
                "key": "d",
                "name": "diff",
                "type": "command",
                "command": "workbench.files.action.compareFileWith"
            },
            {
                "key": "f",
                "name": "fetch",
                "type": "command",
                "command": "git.fetch"
            },
            {
                "key": "i",
                "name": "init",
                "type": "command",
                "command": "git.init"
            },
            {
                "key": "m",
                "name": "merge",
                "type": "command",
                "command": "git.merge"
            },
            {
                "key": "s",
                "name": "status",
                "type": "command",
                "command": "workbench.view.scm"
            },
            {
                "key": "u",
                "name": "unstage",
                "type": "command",
                "command": "git.unstage"
            }
        ]
    },
    {
        "key": "l",
        "name": "+LSP...",
        "type": "bindings",
        "bindings": [
            {
                "key": "d",
                "name": "definition",
                "type": "command",
                "command": "editor.action.revealDefinition" 
            },
            {
                "key": "f",
                "name": "format",
                "type": "command",
                "command": "editor.action.formatSelection",
                "when": "editorHasDocumentSelectionFormattingProvider && editorTextFocus && !editorReadonly"
            },
            {
                "key": "q",
                "name": "quickfix",
                "type": "command",
                "command": "editor.action.quickFix"
            },
            {
                "key": "r",
                "name": "rename",
                "type": "command",
                "command": "editor.action.rename",
                "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
            },
            {
                "key": "R",
                "name": "references",
                "type": "command",
                "command": "editor.action.referenceSearch.trigger"
            },
            {

                "key": "m",
                "name": "method",
                "type": "command",
                "command": "intelli-refactor.extract.function",
                "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly"
            }
        ]
    },
    {
        "key": "m",
        "name": "+MARKS...",
        "type": "bindings",
        "bindings": [
            {
                "key": "c",
                "name": "Clear Bookmarks",
                "type": "command",
                "command": "bookmarks.clear"
            },
            {
                "key": "l",
                "name": "List Bookmarks",
                "type": "command",
                "command": "bookmarks.listFromAllFiles",
                "when": "editorTextFocus"
            },
            {
                "key": "n",
                "name": "Next Bookmark",
                "type": "command",
                "command": "bookmarks.jumpToNext",
                "when": "editorTextFocus"
            },
            {
                "key": "N",
                "name": "Previous Bookmark",
                "type": "command",
                "command": "bookmarks.jumpToPrevious",
                "when": "editorTextFocus"
            },
            {
                "key": "r",
                "name": "Refresh Bookmarks",
                "type": "command",
                "command": "bookmarks.refresh"
            },
            {
                "key": "s",
                "name": "Show Bookmarks",
                "type": "command",
                "command": "workbench.view.extension.bookmarks"
            },
            {
                "key": "t",
                "name": "Toggle Bookmark",
                "type": "command",
                "command": "bookmarks.toggle",
                "when": "editorTextFocus"
            }
        ]
    },
    {
        "key": "s",
        "name": "+SEARCH...",
        "type": "bindings",
        "bindings": [
            {
                "key": "f",
                "name": "file",
                "type": "command",
                "command": "workbench.action.quickOpen"
            },
            {
                "key": "t",
                "name": "text Rg",
                "type": "command",
                "command": "workbench.action.findInFiles"
            }
        ]
    },
    {
        "key": "w",
        "name": "+WINDOW...",
        "type": "bindings",
        "bindings": [
            {
                "key": "s",
                "name": "split horizontal",
                "type": "command",
                "command": "workbench.action.splitEditorDown"
            },
            {
                "key": "v",
                "name": "split vertical",
                "type": "command",
                "command": "workbench.action.splitEditor"
            },
            {
                "key": "t",
                "name": "toggle terminal",
                "type": "command",
                "command": "workbench.action.terminal.toggleTerminal",
                "when": "terminal.active"
            },
            {
                "key": "=",
                "name": "reset editor group sizes",
                "type": "command",
                "command": "workbench.action.evenEditorWidths"
            },
        ]
    }
],


