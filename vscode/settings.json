{
    "terminal.integrated.fontFamily": "Jetbrains Mono",
    "editor.fontFamily": "Jetbrains Mono",
    "editor.fontLigatures": true,
    "editor.inlayHints.fontFamily": "Jetbrains Mono",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "latex-workshop.view.pdf.viewer": "tab",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "search.useIgnoreFiles": false,



    "editor.suggestSelection": "first",
    "editor.rulers": [80],
    "editor.wrappingIndent": "same",
    "editor.tabCompletion": "on",
    
    


    "vim.startofline": false,
    "vim.hlsearch": true,
    "vim.vimrc.enable": true,
    "vim.leader": ",",
    "vim.enableNeovim": true,
    "vim.neovimPath": "/usr/local/bin/nvim",
		"vim.handleKeys": {
			"<C-b>": false,
			"<C-w>": false,
			"<C-d>": false
		},
    "vim.insertModeKeyBindings": [
            {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
],
"vim.visualModeKeyBindingsNonRecursive": [
    {
        "before": [">"],
        "after": [">","g","v"]
    },
    {
        "before": ["<"],
        "after": ["<", "g","v"]
    }
],
"vim.normalModeKeyBindingsNonRecursive": [
    {
        "before": ["j"],
        "after": ["g", "j"]
    },
    {
        "before": ["k"],
        "after": ["g", "k"]
    },
    {
        "before": [ "-" ],
        "after": [ "/" ]
    },
    {
        "before": ["y","p"],
        "after": ["\"","0","p"]
    },
    {
        "before": ["g","d"],
        "commands": ["editor.action.revealDefinition"]
    },
    {
        "before": ["g","f"],
        "commands": ["editor.action.openLink"]
    },
    {
        "before": ["]","g"],
        "commands": ["editor.action.marker.next"]
    
    },
    {
        "before": ["[","g"],
        "commands": ["editor.action.marker.prev"]
    
    },
    {
        "before": [">"],
        "commands": ["editor.action.indentLines"]
    },
    {
        "before": ["<"],
        "commands": ["editor.action.outdentLines"]
    }
],
"vim.highlightedyank.enable": true,
"vim.searchHighlightTextColor": "rgb(0,0,0)",
"vim.searchHighlightColor": "rgb(255,255,0)",
"vim.showMarksInGutter": true,
"vim.showmodename": true,
"vim.useSystemClipboard": true,
"vim.visualstar": true,


    "python.languageServer": "Pylance",
    "debug.javascript.autoAttachFilter": "smart",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "editor.tabSize": 2,
    "editor.detectIndentation": false,
    "files.associations": {
        "*.hs": "haskell"
    },
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-11",
            "path": "/usr/lib/jvm/java-11-openjdk",
            "default": true
        }
    ],
    "java.home": "/usr/lib/jvm/java-11-openjdk",
    "workbench.colorTheme": "Gruvbox Material Dark",
    "workbench.colorCustomizations": {
        "editor.selectionBackground": "#8a8787",
        "editor.selectionHighlightBackground": "#8a8787"
    },
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "workbench.iconTheme": "material-icon-theme",
    "csharp.semanticHighlighting.enabled": true,
    "workbench.productIconTheme": "material-product-icons",
    "projectManager.git.baseFolders": [
        "~/hsr/epj/api",
        "~/hsr/epj/documentation",
        "~/hsr/epj/frontend",
        "~/hsr/epj/runner",
        "~/hsr/ds1/ds1"
    ],
    "peacock.favoriteColors": [
        {
            "name": "Angular Red",
            "value": "#dd0531"
        },
        {
            "name": "Azure Blue",
            "value": "#007fff"
        },
        {
            "name": "JavaScript Yellow",
            "value": "#f9e64f"
        },
        {
            "name": "Mandalorian Blue",
            "value": "#1857a4"
        },
        {
            "name": "Node Green",
            "value": "#215732"
        },
        {
            "name": "React Blue",
            "value": "#61dafb"
        },
        {
            "name": "Something Different",
            "value": "#832561"
        },
        {
            "name": "Svelte Orange",
            "value": "#ff3d00"
        },
        {
            "name": "Vue Green",
            "value": "#42b883"
        }
    ],
    "window.zoomLevel": 1,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.cursorStyle": "line",
    "editor.insertSpaces": false,
    "editor.lineNumbers": "on",
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "editor.wordWrap": "off",
    "vim.easymotion": true,
}