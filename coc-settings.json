{
  "suggest.completionItemKindLabels": {
    "text": "t",
    "method": "Meth",
    "function": "Func",
    "constructor": "Con",
    "field": "f",
    "variable": "Var",
    "class": "Class",
    "interface": "I",
    "module": "m",
    "property": "Prop",
    "unit": "u",
    "value": "val",
    "enum": "e",
    "keyword": "k",
    "snippet": "﬌",
    "color": "",
    "file": "",
    "reference": "ref",
    "folder": "'",
    "enumMember": "",
    "constant": "const",
    "struct": "",
    "event": "event",
    "operator": "op",
    "typeParameter": "tp"
  },
  "snippets.priority": 1,
  "suggest.languageSourcePriority": 99,
  "suggest.enablePreselect": false,
  "suggest.noselect": false,
  "coc.source.file.priority": 2,
  "coc.source.around.priority": 3,
  "coc.source.buffer.priority": 4,

  // General
  "diagnostic.errorSign": " ",
  "diagnostic.warningSign": " ",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": " ",

  // codelens
  "codeLens.enable": true,

  // snippets
  "snippets.userSnippetsDirectory": "~/.config/nvim/snips",

  // list
  "list.indicator": ">",
  "list.selectedSignText": " ",
  // c++
  "clangd.semanticHighlighting": true,

  // Java
  "java.referencesCodeLens.enabled": true,
  "java.implementationsCodeLens.enabled": true,
  "java.completion.enabled": true,

  // explorer
  "explorer.width": 30,
  "explorer.file.autoReveal": false,
  "explorer.showHiddenFiles": true,
  "explorer.buffer.root.template": "[icon & 1] OPEN EDITORS",
  "explorer.file.root.template": "[icon & 1] PROJECT ([root])",
  "explorer.file.child.template": "[git | 2] [selection | clip | 1] [indent][icon | 1] [diagnosticError & 1][filename omitCenter 1][modified][readonly] [linkIcon & 1][link growRight 1 omitCenter 5]",
  "explorer.icon.enableNerdfont": false,
  "explorer.icon.enableVimDevicons": true,
  "explorer.previewAction.onHover": false,
  "explorer.keyMappings.global": {
    "<cr>": [
      "expandable?",
      "expand",
      "open"
    ],
    "v": "open:vsplit"
  },
  "explorer.git.icon.status.added": "A",
  "explorer.git.icon.status.modified": "M",
  "explorer.git.icon.status.deleted": "D",
  "explorer.git.icon.status.untracked": "UT"
}
